import turtle as tur
s=tur.Screen()
t=tur.Turtle()
s.bgcolor("black")
t.shape("turtle")
t.color("red")
t.speed(None)
t.fillcolor("red")
t.begin_fill()
t.left(t.towards(-150,200))
t.forward(200)
t.setheading(90)
for i in range(180):
    t.forward(1)
    t.right(1)
t.setheading(90)
for i in range(180):
    t.forward(1)
    t.right(1)
t.goto(0,0)
t.end_fill()
t.setheading(90)
t.goto(0,20) 
